  "info": {
    "version": "2.0.0",
    "title": "Most Popular",
    "description": "Provides services for getting the most popular articles on NYTimes.com based on emails, shares, or views.\n\nGet most emailed articles for the last day:\n```\n/emailed/1.json\n```\n\nGet most shared articles on Facebook for the last day:\n```\n/shared/1/facebook.json\n```\n\nGet most viewed articles for the last seven days:\n```\n/viewed/7.json\n```\n\n## Example Calls\n```\nhttps://api.nytimes.com/svc/mostpopular/v2/emailed/7.json?api-key=yourkey\n```\n\n```\nhttps://api.nytimes.com/svc/mostpopular/v2/shared/1/facebook.json?api-key=yourkey\n```\n\n```\nhttps://api.nytimes.com/svc/mostpopular/v2/viewed/1.json?api-key=yourkey\n```\n"
  },
  "produces": [
    "application/json"
  ],
  "security": [
    {
      "apikey": [

      ]
    }
  ],
  "paths": {
    "/emailed/{period}.json": {
      "get": {
        "summary": "Most emailed articles on NYTimes.com.",
        "description": "Returns an array of the most emailed articles on NYTimes.com for specified period of time (1 day, 7 days, or 30 days).\n",
        "parameters": [
          {
            "name": "period",
            "in": "path",
            "description": "Time period: 1, 7, or 30 days.",
            "required": true,
            "type": "integer",
            "enum": [
              1,
              7,
              30
            ],
            "default": 1
          }
        ],
        "tags": [
          "Most Popular"
        ],
        "responses": {
          "200": {
            "description": "An array of articles.",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "description": "API response status (e.g. OK)."
                },
                "copyright": {
                  "type": "string",
                  "description": "Copyright message."
                },
                "num_results": {
                  "type": "integer",
                  "description": "Number of articles in the results (e.g. 20)."
                },
                "results": {
                  "type": "array",
                  "items": {
                    "$ref": "#/definitions/EmailedArticle"
                  },
                  "description": "Array of articles."
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized request. Make sure api-key is set."
          },
          "429": {
            "description": "Too many requests. You reached your per minute or per day rate limit."
          }
        }
      }
    },
